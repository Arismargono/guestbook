{
  "version": 2,
  "builds": [
    {
      "src": "api/[[...path]].js",
      "use": "@vercel/node"
    },
    {
      "src": "api/health.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/api/health",
      "dest": "/api/health.js"
    },
    {
      "src": "/api/(.*)",
      "dest": "/api/[[...path]].js"
    },
    {
      "src": "/photos/(.*)",
      "dest": "/backend/photos/$1"
    },
    {
      "src": "/assets/(.*)",
      "dest": "/dist/assets/$1"
    },
    {
      "src": "/(.*)",
      "dest": "/api/[[...path]].js"
    }
  ]
}